["\u7121", "\u611f\u89ba\u7684\u51fa\u8001\u5e2b\u7684\u6559\u5b78\u71b1\u8aa0\uff0c\u4f46\u4e0a\u8ab2\u5167\u5bb9\u6709\u4e9b\u7121\u8da3\uff0c\u8207\u5176\u4ed6\u7cfb\u6240\u7684\u7d93\u6fdf\u8ab2\u7a0b\u53c8\u5dee\u7570\u9817\u5927\uff0c\u7121\u6cd5\u6311\u8d77\u5b78\u751f\u5b78\u7fd2\u7684\u8208\u8da3\uff0c\u8001\u5e2b\u4eba\u6eab\u548c\u53cb\u5584\uff0c\u53ea\u662f\u6559\u5b78\u4e0a\u8ab2\u53ef\u80fd\u8981\u518d\u8abf\u6574\uff0c\u7d93\u6fdf\u5b78\u5c0d\u65bc\u6cd5\u5f8b\u7cfb\u5b78\u751f\u662f\u96de\u808b\u7684\u8ab2\u7a0b\uff0c\u5982\u4f55\u589e\u5f37\u5b78\u751f\u5b78\u7fd2\u7684\u52d5\u6a5f\uff0c\u53ef\u4ee5\u597d\u597d\u601d\u8003", "\u8001\u5e2b\u8ddf\u52a9\u6559\u90fd\u5f88\u7528\u5fc3", "\u8001\u5e2b\u8a8d\u771f\u8ca0\u8cac\uff0c\u4f46\u500b\u4eba\u5728\u5b78\u7fd2\u4e0a\u9084\u662f\u6709\u4e9b\u529b\u4e0d\u5f9e\u5fc3\u3002", "\u7121", "\u5167\u5bb9\u6709\u9ede\u7121\u804a\uff0c\u6709\u6642\u5019\u6703\u4e0d\u77e5\u9053\u81ea\u5df1\u5b78\u4e86\u4ec0\u9ebc", "\u7121", "\u5e0c\u671b\u8001\u5e2b\u4e0d\u8981\u4e00\u76f4\u9ede\u540d\uff08\u4e0a\u8ab2\u7dda\u4e0a\u4f5c\u7b54\u597d\u5e7e\u6b21\u7684\u90e8\u5206\uff09", "\u8001\u5e2b\u4e0a\u8ab2\u5f88\u6709\u71b1\u60c5", "\u674e\u8001\u5e2b\u7684\u6559\u5b78\u65b9\u5f0f\u53ef\u4ee5\u9069\u5ea6\u8abf\u6574\u4ee5\u63d0\u9ad8\u5b78\u751f\u8208\u8da3", "\u7121", "\u4e0a\u8ab2\u6b65\u9a5f\u6709\u4e00\u9ede\u5feb\uff0c\u9084\u5728\u601d\u8003\u4e0a\u4e00\u6bb5\uff0c\u5c31\u8df3\u5230\u4e0b\u4e00\u6bb5\u4e86\uff0c\u5efa\u8b70\u53ef\u4ee5\u4e0a\u6162\u4e00\u9ede\uff0c\u8001\u8a69\u4eba\u5f88\u597d\uff0c\u5f88\u71b1\u65bc\u70ba\u5b78\u751f\u89e3\u7b54\u554f\u984c!", "\u8001\u5e2b\u5c0d\u8ab2\u7a0b\u9032\u5ea6\u6709\u5f88\u597d\u7684\u638c\u63e1", "\u4e0a\u8ab2\u53ef\u66f4\u6d3b\u7528\u4e00\u9ede\uff0c\u4e0d\u9700\u6b7b\u677f\u8b1b\u89e3\u8ab2\u672c\u77e5\u8b58\uff0c\u5bb9\u6613\u8b93\u5b78\u751f\u5931\u53bb\u5b78\u7fd2\u52d5\u529b\u3002", "\u4e0a\u8ab2\u8b1b\u89e3\u807d\u4e0d\u592a\u61c2\uff0c\u800c\u6559\u6750\u7de8\u6392\u4e5f\u4e0d\u662f\u5f88\u6613\u61c2\uff0c\u8f03\u96e3\u7406\u89e3", "\u5e0c\u671b\u8001\u5e2b\u8b1b\u89e3\u53ef\u4ee5\u66f4\u6e05\u695a\uff5e", "\u570b\u6a11\u548c\u70ba\u6968\u8001\u5e2b\u4e0a\u8ab2\u90fd\u5f88\u8a8d\u771f\uff0c\u52a9\u6559\u6539\u8003\u5377\u7d66\u5206\u5f88\u751c\uff0c\u4f46\u662f\u6211\u89ba\u5f97\u6211\u81ea\u5df1\u5438\u6536\u5f97\u4e0d\u592a\u597d\uff5e", "\u8001\u5e2b\u8b1b\u7684\u7b97\u6e05\u695a\uff0c\u8003\u8a66\u4e5f\u4e0d\u592a\u96e3\uff0c\u53ea\u662f\u5c0d\u65bc\u7d93\u6fdf\u60f3\u6709\u66f4\u6df1\u5165\u7684\u5b78\u7fd2\u81ea\u5df1\u5fc5\u9808\u591a\u4e0b\u82e6\u529f\uff0c\u7562\u7adf\u53ea\u6709\u5169\u5b78\u5206\u7684\u8ab2\uff0c\u8ddf\u5176\u4ed6\u7cfb\u6240\u958b\u7684\u9084\u662f\u6709\u4e9b\u5dee\u8ddd\u3002", "\u53ef\u4ee5\u8b1b\u4e00\u4e0b\u8207\u6295\u8cc7\u76f8\u95dc\u7684\u4e8b\u52d9\u3002", "\u5e0c\u671b\u53ef\u4ee5\u6559\u5f97\u6e05\u695a\u4e00\u4e9b  \u6642\u5e38\u807d\u4e0d\u592a\u61c2\u56de\u53bb\u5ff5\u66f8\u4e5f\u770b\u4e0d\u592a\u61c2", "\u7121", "\u8001\u5e2b\u4e0a\u8ab2\u5f88\u8a8d\u771f\uff0c\u4e0d\u904e\u5c31\u5716\u5f62\u7684\u90e8\u5206\u53ef\u4ee5\u518d\u8b1b\u89e3\u7684\u8a73\u7d30\u4e00\u9ede\uff0c\u90e8\u5206\u5b78\u751f\u597d\u50cf\u4e00\u77e5\u534a\u89e3\u3002", "\u611f\u8b1d\u8001\u5e2b\uff01\u8f9b\u82e6\u4e86\uff01", "\u5e0c\u671b\u8001\u5e2b\u53ef\u4ee5\u8aaa\u7684\u6709\u689d\u7406\u4e14\u6e05\u695a\u4e00\u9ede", "\u8a0e\u53ad\u7528wm5\u9ede\u540d\uff0c\u56de\u7b54\u554f\u984c\u3002\u53ef\u7d50\u5408\u751f\u6d3b\u5be6\u4f8b\u3002", "\u8003\u8a66\u984c\u76ee\u53ef\u4ee5\u7c21\u55ae\u4e00\u9ede"]["\u674e\u8001\u5e2b\u7684\u6559\u5b78\u65b9\u5f0f\u53ef\u4ee5\u9069\u5ea6\u8abf\u6574\u4ee5\u63d0\u9ad8\u5b78\u751f\u8208\u8da3", "\u8001\u5e2b\u8ddf\u52a9\u6559\u90fd\u5f88\u7528\u5fc3", "\u5e0c\u671b\u53ef\u4ee5\u6559\u5f97\u6e05\u695a\u4e00\u4e9b  \u6642\u5e38\u807d\u4e0d\u592a\u61c2\u56de\u53bb\u5ff5\u66f8\u4e5f\u770b\u4e0d\u592a\u61c2", "\u8001\u5e2b\u8a8d\u771f\u8ca0\u8cac\uff0c\u4f46\u500b\u4eba\u5728\u5b78\u7fd2\u4e0a\u9084\u662f\u6709\u4e9b\u529b\u4e0d\u5f9e\u5fc3\u3002", "\u7121", "\u4e0a\u8ab2\u53ef\u66f4\u6d3b\u7528\u4e00\u9ede\uff0c\u4e0d\u9700\u6b7b\u677f\u8b1b\u89e3\u8ab2\u672c\u77e5\u8b58\uff0c\u5bb9\u6613\u8b93\u5b78\u751f\u5931\u53bb\u5b78\u7fd2\u52d5\u529b\u3002", "\u611f\u89ba\u7684\u51fa\u8001\u5e2b\u7684\u6559\u5b78\u71b1\u8aa0\uff0c\u4f46\u4e0a\u8ab2\u5167\u5bb9\u6709\u4e9b\u7121\u8da3\uff0c\u8207\u5176\u4ed6\u7cfb\u6240\u7684\u7d93\u6fdf\u8ab2\u7a0b\u53c8\u5dee\u7570\u9817\u5927\uff0c\u7121\u6cd5\u6311\u8d77\u5b78\u751f\u5b78\u7fd2\u7684\u8208\u8da3\uff0c\u8001\u5e2b\u4eba\u6eab\u548c\u53cb\u5584\uff0c\u53ea\u662f\u6559\u5b78\u4e0a\u8ab2\u53ef\u80fd\u8981\u518d\u8abf\u6574\uff0c\u7d93\u6fdf\u5b78\u5c0d\u65bc\u6cd5\u5f8b\u7cfb\u5b78\u751f\u662f\u96de\u808b\u7684\u8ab2\u7a0b\uff0c\u5982\u4f55\u589e\u5f37\u5b78\u751f\u5b78\u7fd2\u7684\u52d5\u6a5f\uff0c\u53ef\u4ee5\u597d\u597d\u601d\u8003", "\u8a0e\u53ad\u7528wm5\u9ede\u540d\uff0c\u56de\u7b54\u554f\u984c\u3002\u53ef\u7d50\u5408\u751f\u6d3b\u5be6\u4f8b\u3002", "\u7121", "\u7121", "\u4e0a\u8ab2\u6b65\u9a5f\u6709\u4e00\u9ede\u5feb\uff0c\u9084\u5728\u601d\u8003\u4e0a\u4e00\u6bb5\uff0c\u5c31\u8df3\u5230\u4e0b\u4e00\u6bb5\u4e86\uff0c\u5efa\u8b70\u53ef\u4ee5\u4e0a\u6162\u4e00\u9ede\uff0c\u8001\u8a69\u4eba\u5f88\u597d\uff0c\u5f88\u71b1\u65bc\u70ba\u5b78\u751f\u89e3\u7b54\u554f\u984c!", "\u5e0c\u671b\u8001\u5e2b\u53ef\u4ee5\u8aaa\u7684\u6709\u689d\u7406\u4e14\u6e05\u695a\u4e00\u9ede", "\u8001\u5e2b\u5c0d\u8ab2\u7a0b\u9032\u5ea6\u6709\u5f88\u597d\u7684\u638c\u63e1", "\u570b\u6a11\u548c\u70ba\u6968\u8001\u5e2b\u4e0a\u8ab2\u90fd\u5f88\u8a8d\u771f\uff0c\u52a9\u6559\u6539\u8003\u5377\u7d66\u5206\u5f88\u751c\uff0c\u4f46\u662f\u6211\u89ba\u5f97\u6211\u81ea\u5df1\u5438\u6536\u5f97\u4e0d\u592a\u597d\uff5e", "\u8001\u5e2b\u4e0a\u8ab2\u5f88\u8a8d\u771f\uff0c\u4e0d\u904e\u5c31\u5716\u5f62\u7684\u90e8\u5206\u53ef\u4ee5\u518d\u8b1b\u89e3\u7684\u8a73\u7d30\u4e00\u9ede\uff0c\u90e8\u5206\u5b78\u751f\u597d\u50cf\u4e00\u77e5\u534a\u89e3\u3002", "\u611f\u8b1d\u8001\u5e2b\uff01\u8f9b\u82e6\u4e86\uff01", "\u5e0c\u671b\u8001\u5e2b\u4e0d\u8981\u4e00\u76f4\u9ede\u540d\uff08\u4e0a\u8ab2\u7dda\u4e0a\u4f5c\u7b54\u597d\u5e7e\u6b21\u7684\u90e8\u5206\uff09", "\u4e0a\u8ab2\u8b1b\u89e3\u807d\u4e0d\u592a\u61c2\uff0c\u800c\u6559\u6750\u7de8\u6392\u4e5f\u4e0d\u662f\u5f88\u6613\u61c2\uff0c\u8f03\u96e3\u7406\u89e3", "\u5167\u5bb9\u6709\u9ede\u7121\u804a\uff0c\u6709\u6642\u5019\u6703\u4e0d\u77e5\u9053\u81ea\u5df1\u5b78\u4e86\u4ec0\u9ebc", "\u8001\u5e2b\u8b1b\u7684\u7b97\u6e05\u695a\uff0c\u8003\u8a66\u4e5f\u4e0d\u592a\u96e3\uff0c\u53ea\u662f\u5c0d\u65bc\u7d93\u6fdf\u60f3\u6709\u66f4\u6df1\u5165\u7684\u5b78\u7fd2\u81ea\u5df1\u5fc5\u9808\u591a\u4e0b\u82e6\u529f\uff0c\u7562\u7adf\u53ea\u6709\u5169\u5b78\u5206\u7684\u8ab2\uff0c\u8ddf\u5176\u4ed6\u7cfb\u6240\u958b\u7684\u9084\u662f\u6709\u4e9b\u5dee\u8ddd\u3002", "\u8003\u8a66\u984c\u76ee\u53ef\u4ee5\u7c21\u55ae\u4e00\u9ede", "\u5e0c\u671b\u8001\u5e2b\u8b1b\u89e3\u53ef\u4ee5\u66f4\u6e05\u695a\uff5e", "\u7121", "\u8001\u5e2b\u4e0a\u8ab2\u5f88\u6709\u71b1\u60c5", "\u7121", "\u53ef\u4ee5\u8b1b\u4e00\u4e0b\u8207\u6295\u8cc7\u76f8\u95dc\u7684\u4e8b\u52d9\u3002"]["\u674e\u8001\u5e2b\u7684\u6559\u5b78\u65b9\u5f0f\u53ef\u4ee5\u9069\u5ea6\u8abf\u6574\u4ee5\u63d0\u9ad8\u5b78\u751f\u8208\u8da3", "\u8001\u5e2b\u8ddf\u52a9\u6559\u90fd\u5f88\u7528\u5fc3", "\u5e0c\u671b\u53ef\u4ee5\u6559\u5f97\u6e05\u695a\u4e00\u4e9b  \u6642\u5e38\u807d\u4e0d\u592a\u61c2\u56de\u53bb\u5ff5\u66f8\u4e5f\u770b\u4e0d\u592a\u61c2", "\u8001\u5e2b\u8a8d\u771f\u8ca0\u8cac\uff0c\u4f46\u500b\u4eba\u5728\u5b78\u7fd2\u4e0a\u9084\u662f\u6709\u4e9b\u529b\u4e0d\u5f9e\u5fc3\u3002", "\u7121", "\u4e0a\u8ab2\u53ef\u66f4\u6d3b\u7528\u4e00\u9ede\uff0c\u4e0d\u9700\u6b7b\u677f\u8b1b\u89e3\u8ab2\u672c\u77e5\u8b58\uff0c\u5bb9\u6613\u8b93\u5b78\u751f\u5931\u53bb\u5b78\u7fd2\u52d5\u529b\u3002", "\u611f\u89ba\u7684\u51fa\u8001\u5e2b\u7684\u6559\u5b78\u71b1\u8aa0\uff0c\u4f46\u4e0a\u8ab2\u5167\u5bb9\u6709\u4e9b\u7121\u8da3\uff0c\u8207\u5176\u4ed6\u7cfb\u6240\u7684\u7d93\u6fdf\u8ab2\u7a0b\u53c8\u5dee\u7570\u9817\u5927\uff0c\u7121\u6cd5\u6311\u8d77\u5b78\u751f\u5b78\u7fd2\u7684\u8208\u8da3\uff0c\u8001\u5e2b\u4eba\u6eab\u548c\u53cb\u5584\uff0c\u53ea\u662f\u6559\u5b78\u4e0a\u8ab2\u53ef\u80fd\u8981\u518d\u8abf\u6574\uff0c\u7d93\u6fdf\u5b78\u5c0d\u65bc\u6cd5\u5f8b\u7cfb\u5b78\u751f\u662f\u96de\u808b\u7684\u8ab2\u7a0b\uff0c\u5982\u4f55\u589e\u5f37\u5b78\u751f\u5b78\u7fd2\u7684\u52d5\u6a5f\uff0c\u53ef\u4ee5\u597d\u597d\u601d\u8003", "\u8a0e\u53ad\u7528wm5\u9ede\u540d\uff0c\u56de\u7b54\u554f\u984c\u3002\u53ef\u7d50\u5408\u751f\u6d3b\u5be6\u4f8b\u3002", "\u7121", "\u7121", "\u4e0a\u8ab2\u6b65\u9a5f\u6709\u4e00\u9ede\u5feb\uff0c\u9084\u5728\u601d\u8003\u4e0a\u4e00\u6bb5\uff0c\u5c31\u8df3\u5230\u4e0b\u4e00\u6bb5\u4e86\uff0c\u5efa\u8b70\u53ef\u4ee5\u4e0a\u6162\u4e00\u9ede\uff0c\u8001\u8a69\u4eba\u5f88\u597d\uff0c\u5f88\u71b1\u65bc\u70ba\u5b78\u751f\u89e3\u7b54\u554f\u984c!", "\u5e0c\u671b\u8001\u5e2b\u53ef\u4ee5\u8aaa\u7684\u6709\u689d\u7406\u4e14\u6e05\u695a\u4e00\u9ede", "\u8001\u5e2b\u5c0d\u8ab2\u7a0b\u9032\u5ea6\u6709\u5f88\u597d\u7684\u638c\u63e1", "\u570b\u6a11\u548c\u70ba\u6968\u8001\u5e2b\u4e0a\u8ab2\u90fd\u5f88\u8a8d\u771f\uff0c\u52a9\u6559\u6539\u8003\u5377\u7d66\u5206\u5f88\u751c\uff0c\u4f46\u662f\u6211\u89ba\u5f97\u6211\u81ea\u5df1\u5438\u6536\u5f97\u4e0d\u592a\u597d\uff5e", "\u8001\u5e2b\u4e0a\u8ab2\u5f88\u8a8d\u771f\uff0c\u4e0d\u904e\u5c31\u5716\u5f62\u7684\u90e8\u5206\u53ef\u4ee5\u518d\u8b1b\u89e3\u7684\u8a73\u7d30\u4e00\u9ede\uff0c\u90e8\u5206\u5b78\u751f\u597d\u50cf\u4e00\u77e5\u534a\u89e3\u3002", "\u611f\u8b1d\u8001\u5e2b\uff01\u8f9b\u82e6\u4e86\uff01", "\u5e0c\u671b\u8001\u5e2b\u4e0d\u8981\u4e00\u76f4\u9ede\u540d\uff08\u4e0a\u8ab2\u7dda\u4e0a\u4f5c\u7b54\u597d\u5e7e\u6b21\u7684\u90e8\u5206\uff09", "\u4e0a\u8ab2\u8b1b\u89e3\u807d\u4e0d\u592a\u61c2\uff0c\u800c\u6559\u6750\u7de8\u6392\u4e5f\u4e0d\u662f\u5f88\u6613\u61c2\uff0c\u8f03\u96e3\u7406\u89e3", "\u5167\u5bb9\u6709\u9ede\u7121\u804a\uff0c\u6709\u6642\u5019\u6703\u4e0d\u77e5\u9053\u81ea\u5df1\u5b78\u4e86\u4ec0\u9ebc", "\u8001\u5e2b\u8b1b\u7684\u7b97\u6e05\u695a\uff0c\u8003\u8a66\u4e5f\u4e0d\u592a\u96e3\uff0c\u53ea\u662f\u5c0d\u65bc\u7d93\u6fdf\u60f3\u6709\u66f4\u6df1\u5165\u7684\u5b78\u7fd2\u81ea\u5df1\u5fc5\u9808\u591a\u4e0b\u82e6\u529f\uff0c\u7562\u7adf\u53ea\u6709\u5169\u5b78\u5206\u7684\u8ab2\uff0c\u8ddf\u5176\u4ed6\u7cfb\u6240\u958b\u7684\u9084\u662f\u6709\u4e9b\u5dee\u8ddd\u3002", "\u8003\u8a66\u984c\u76ee\u53ef\u4ee5\u7c21\u55ae\u4e00\u9ede", "\u5e0c\u671b\u8001\u5e2b\u8b1b\u89e3\u53ef\u4ee5\u66f4\u6e05\u695a\uff5e", "\u7121", "\u8001\u5e2b\u4e0a\u8ab2\u5f88\u6709\u71b1\u60c5", "\u7121", "\u53ef\u4ee5\u8b1b\u4e00\u4e0b\u8207\u6295\u8cc7\u76f8\u95dc\u7684\u4e8b\u52d9\u3002"]["\u674e\u8001\u5e2b\u7684\u6559\u5b78\u65b9\u5f0f\u53ef\u4ee5\u9069\u5ea6\u8abf\u6574\u4ee5\u63d0\u9ad8\u5b78\u751f\u8208\u8da3", "\u8001\u5e2b\u8ddf\u52a9\u6559\u90fd\u5f88\u7528\u5fc3", "\u5e0c\u671b\u53ef\u4ee5\u6559\u5f97\u6e05\u695a\u4e00\u4e9b  \u6642\u5e38\u807d\u4e0d\u592a\u61c2\u56de\u53bb\u5ff5\u66f8\u4e5f\u770b\u4e0d\u592a\u61c2", "\u8001\u5e2b\u8a8d\u771f\u8ca0\u8cac\uff0c\u4f46\u500b\u4eba\u5728\u5b78\u7fd2\u4e0a\u9084\u662f\u6709\u4e9b\u529b\u4e0d\u5f9e\u5fc3\u3002", "\u7121", "\u4e0a\u8ab2\u53ef\u66f4\u6d3b\u7528\u4e00\u9ede\uff0c\u4e0d\u9700\u6b7b\u677f\u8b1b\u89e3\u8ab2\u672c\u77e5\u8b58\uff0c\u5bb9\u6613\u8b93\u5b78\u751f\u5931\u53bb\u5b78\u7fd2\u52d5\u529b\u3002", "\u611f\u89ba\u7684\u51fa\u8001\u5e2b\u7684\u6559\u5b78\u71b1\u8aa0\uff0c\u4f46\u4e0a\u8ab2\u5167\u5bb9\u6709\u4e9b\u7121\u8da3\uff0c\u8207\u5176\u4ed6\u7cfb\u6240\u7684\u7d93\u6fdf\u8ab2\u7a0b\u53c8\u5dee\u7570\u9817\u5927\uff0c\u7121\u6cd5\u6311\u8d77\u5b78\u751f\u5b78\u7fd2\u7684\u8208\u8da3\uff0c\u8001\u5e2b\u4eba\u6eab\u548c\u53cb\u5584\uff0c\u53ea\u662f\u6559\u5b78\u4e0a\u8ab2\u53ef\u80fd\u8981\u518d\u8abf\u6574\uff0c\u7d93\u6fdf\u5b78\u5c0d\u65bc\u6cd5\u5f8b\u7cfb\u5b78\u751f\u662f\u96de\u808b\u7684\u8ab2\u7a0b\uff0c\u5982\u4f55\u589e\u5f37\u5b78\u751f\u5b78\u7fd2\u7684\u52d5\u6a5f\uff0c\u53ef\u4ee5\u597d\u597d\u601d\u8003", "\u8a0e\u53ad\u7528wm5\u9ede\u540d\uff0c\u56de\u7b54\u554f\u984c\u3002\u53ef\u7d50\u5408\u751f\u6d3b\u5be6\u4f8b\u3002", "\u7121", "\u7121", "\u4e0a\u8ab2\u6b65\u9a5f\u6709\u4e00\u9ede\u5feb\uff0c\u9084\u5728\u601d\u8003\u4e0a\u4e00\u6bb5\uff0c\u5c31\u8df3\u5230\u4e0b\u4e00\u6bb5\u4e86\uff0c\u5efa\u8b70\u53ef\u4ee5\u4e0a\u6162\u4e00\u9ede\uff0c\u8001\u8a69\u4eba\u5f88\u597d\uff0c\u5f88\u71b1\u65bc\u70ba\u5b78\u751f\u89e3\u7b54\u554f\u984c!", "\u5e0c\u671b\u8001\u5e2b\u53ef\u4ee5\u8aaa\u7684\u6709\u689d\u7406\u4e14\u6e05\u695a\u4e00\u9ede", "\u8001\u5e2b\u5c0d\u8ab2\u7a0b\u9032\u5ea6\u6709\u5f88\u597d\u7684\u638c\u63e1", "\u570b\u6a11\u548c\u70ba\u6968\u8001\u5e2b\u4e0a\u8ab2\u90fd\u5f88\u8a8d\u771f\uff0c\u52a9\u6559\u6539\u8003\u5377\u7d66\u5206\u5f88\u751c\uff0c\u4f46\u662f\u6211\u89ba\u5f97\u6211\u81ea\u5df1\u5438\u6536\u5f97\u4e0d\u592a\u597d\uff5e", "\u8001\u5e2b\u4e0a\u8ab2\u5f88\u8a8d\u771f\uff0c\u4e0d\u904e\u5c31\u5716\u5f62\u7684\u90e8\u5206\u53ef\u4ee5\u518d\u8b1b\u89e3\u7684\u8a73\u7d30\u4e00\u9ede\uff0c\u90e8\u5206\u5b78\u751f\u597d\u50cf\u4e00\u77e5\u534a\u89e3\u3002", "\u611f\u8b1d\u8001\u5e2b\uff01\u8f9b\u82e6\u4e86\uff01", "\u5e0c\u671b\u8001\u5e2b\u4e0d\u8981\u4e00\u76f4\u9ede\u540d\uff08\u4e0a\u8ab2\u7dda\u4e0a\u4f5c\u7b54\u597d\u5e7e\u6b21\u7684\u90e8\u5206\uff09", "\u4e0a\u8ab2\u8b1b\u89e3\u807d\u4e0d\u592a\u61c2\uff0c\u800c\u6559\u6750\u7de8\u6392\u4e5f\u4e0d\u662f\u5f88\u6613\u61c2\uff0c\u8f03\u96e3\u7406\u89e3", "\u5167\u5bb9\u6709\u9ede\u7121\u804a\uff0c\u6709\u6642\u5019\u6703\u4e0d\u77e5\u9053\u81ea\u5df1\u5b78\u4e86\u4ec0\u9ebc", "\u8001\u5e2b\u8b1b\u7684\u7b97\u6e05\u695a\uff0c\u8003\u8a66\u4e5f\u4e0d\u592a\u96e3\uff0c\u53ea\u662f\u5c0d\u65bc\u7d93\u6fdf\u60f3\u6709\u66f4\u6df1\u5165\u7684\u5b78\u7fd2\u81ea\u5df1\u5fc5\u9808\u591a\u4e0b\u82e6\u529f\uff0c\u7562\u7adf\u53ea\u6709\u5169\u5b78\u5206\u7684\u8ab2\uff0c\u8ddf\u5176\u4ed6\u7cfb\u6240\u958b\u7684\u9084\u662f\u6709\u4e9b\u5dee\u8ddd\u3002", "\u8003\u8a66\u984c\u76ee\u53ef\u4ee5\u7c21\u55ae\u4e00\u9ede", "\u5e0c\u671b\u8001\u5e2b\u8b1b\u89e3\u53ef\u4ee5\u66f4\u6e05\u695a\uff5e", "\u7121", "\u8001\u5e2b\u4e0a\u8ab2\u5f88\u6709\u71b1\u60c5", "\u7121", "\u53ef\u4ee5\u8b1b\u4e00\u4e0b\u8207\u6295\u8cc7\u76f8\u95dc\u7684\u4e8b\u52d9\u3002"]